
public class Example {
	
	private String name;
	private int age;
	private String address;
	 
	//Constructor1
	public Example() {
		//Default
	}
	
	//Constructor2
	public Example(String name, int age) {
		this.setName(name);
		this.age=age;
	}
	
	//Constructor3
	public Example(String name, int age, String address) {
		this.setName(name);
		this.age=age;
		this.address=address;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}
}
